{
	"version": "1.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/src/server.ts",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}/dist",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/dist"
		},
		{
			"name": "Attach to server",
			"type": "node",
			"request": "attach",
			"port": 41234
		}
	]
}